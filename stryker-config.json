{
  "stryker-config": {
    "mutation-level": "Complete",
    "language-version": "latest",
    "additional-timeout": 5000,
    "mutate": [
      "**/*",
      "!**/*.g.cs",
      "!**/*.generated.cs"
    ],
    "solution": "mississippi.sln",
    "coverage-analysis": "perTest",
    "disable-bail": false,
    "disable-mix-mutants": false,
    "thresholds": {
      "high": 80,
      "low": 60,
      "break": 50
    },
    "verbosity": "info",
    "reporters": [
      "Progress",
      "Html",
      "json",
      "markdown"
    ],
    "since": {
      "enabled": false,
      "ignore-changes-in": [],
      "target": "origin/main"
    },
    "baseline": {
      "enabled": false,
      "provider": "disk",
      "fallback-version": "origin/main"
    },
    "dashboard-url": "https://dashboard.stryker-mutator.io",
    "test-projects": [],
    "test-case-filter": "",
    "ignore-mutations": ["String"],
    "ignore-methods": [],
    "report-file-name": "mutation-report",
    "break-on-initial-test-failure": false
  }
}

